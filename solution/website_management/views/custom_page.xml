<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="view_custom_model_tree" model="ir.ui.view">
        <field name="name">custom.website.tree</field>
        <field name="model">custom.website</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_custom_model" model="ir.actions.act_window">
        <field name="name">Custom Website</field>
        <field name="res_model">custom.website</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>